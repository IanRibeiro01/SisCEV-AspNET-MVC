@model SisCEV_Modelo.Cadastros.Produto

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "SisCEV - Inserir Produtos";
}


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="panel panel-primary">
        <div class="panel-heading">
            Inserir Novo Produto
        </div>
        <div class="panel-body">
                @Html.Partial("~/Areas/Cadastros/Views/Produtos/_PartialCreateContentPanel.cshtml", Model)
        </div>
        <div class="panel-footer panel-info">
            <div class="col-md-10">
                <input type="submit" value="Adicionar Produto" class="btn btn-primary" />
                @Html.ActionLink("Retornar para listagem", "Index", null, new { @class = "btn btn-warning" })
            </div>
            <br />
            <br />
        </div>

    </div>
}

@section styles{
        @*<link href="~/Scripts/jquery-ui-1.12.1.custom/jquery-ui.css" rel="stylesheet" />*@
    }

@section ScriptPage {
    @*<script src="~/Scripts/jquery-ui-1.12.1.custom/jquery-ui.js"></script>*@
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <script type="text/javascript">
            $(document).ready(function () {
                $('li').removeClass("active");
                $('#liProdutos').addClass("active");
             });
    </script>

    }

