@model SisCEV_Modelo.Cadastros.Produto

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "SisCEV - Detalhes do Produto";
}

<div class="panel panel-primary">
    <div class="panel-heading">
        Detalhes do Produto
    </div>

    <div class="panel-body">

        @Html.Partial("~/Views/Produtos/_PartialDetailsContentPanel.cshtml", Model)

    </div>

    <div class="panel-footer panel-info">
        <div class="col-md-10">
            @Html.ActionLink("Alterar", "Edit", new { id = Model.ProdutoID }, new { @class = "btn btn-primary" })
            @Html.ActionLink("Retornar para listagem", "Index", null, new { @class = "btn btn-warning" })
        </div>
        <br />
        <br />
    </div>
</div>

@section ScriptPage {
    <script type="text/javascript">
        $(document).ready(function () {
            $('li').removeClass("active");
            $('#liProdutos').addClass("active");
        });
    </script>
}
